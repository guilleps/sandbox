{
	"branches": ["main"],
	"tagFormat": "frontend@${version}",
	"parserOpts": {
		"noteKeywords": ["BREAKING CHANGE", "BREAKING CHANGES"]
	},
	"presetConfig": {
		"types": [
			{
				"type": "feat",
				"section": "ğŸš€ Features"
			},
			{
				"type": "fix",
				"section": "ğŸ Bug Fixes"
			},
			{
				"type": "docs",
				"section": "ğŸ“ Documentation"
			},
			{
				"type": "style",
				"section": "ğŸ¨ Styles"
			},
			{
				"type": "refactor",
				"section": "â™»ï¸ Refactors"
			},
			{
				"type": "perf",
				"section": "âš¡ Performance"
			},
			{
				"type": "test",
				"section": "ğŸ§ª Tests"
			},
			{
				"type": "build",
				"section": "ğŸ“¦ Build"
			},
			{
				"type": "ci",
				"section": "ğŸ¤– CI"
			},
			{
				"type": "chore",
				"section": "ğŸ§¹ Chores"
			}
		]
	},
	"plugins": [
		[
			"@semantic-release/commit-analyzer",
			{
				"preset": "angular"
			}
		],
		"@semantic-release/release-notes-generator",
		"@semantic-release/changelog",
		"@semantic-release/npm",
		"@semantic-release/github",
		[
			"@semantic-release/git",
			{
				"assets": ["CHANGELOG.md", "package.json"],
				"message": "chore(release): frontend v${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
			}
		]
	]
}
